package TCSS;
import java.util.Scanner;
class A
{
	int a;
	int b;
	
	public void show()
	{
	}
}
class B extends A
{
	B(int a,int b)
	{
		this.a=a;
		this.b=b;
	}
	public void show()
	{
		System.out.println(a+b);
	}
}
class C extends A
{
	int c;
	C(int a,int b,int c)
	{
		this.a=a;
		this.b=b;
		this.c=c;
	}
	public void show()
	{
		System.out.println(a+b+c);
	}
}
class D extends A
{
	int c;
	int d;
	D(int a,int b,int c,int d)
	{
		this.a=a;
		this.b=b;
		this.c=c;
		this.d=d;
	}
	public void show()
	{
		System.out.println(a+b+c+d);
	}

}
public class PractMarch {
	public static void main(String[] args) {
		A obj;
		obj=new B(20,10);
		obj.show();
		obj=new C(40,30,20);
		obj.show();
		obj=new D(40,30,20,10);
		obj.show();
		
		
				
}}